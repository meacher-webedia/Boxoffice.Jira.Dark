{
    "manifest_version": 2,
  
    "name": "The Boxoffice Company Jira Dark Theme",
    "description": "This will so something special.",
    "version": "1.0",
    "icons": {
        "128": "icon.png"
    },
    "browser_action": {
     "default_icon": "icon.png"
    },
    "permissions": [
        "tabs", 
        "https://jira.webedia-group.net/*"
     ],
     "content_scripts": [
        {
          "matches": ["https://jira.webedia-group.net/*"], 
          "css" : ["styles.css"]
        }
    ]
  }